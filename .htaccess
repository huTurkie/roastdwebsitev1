RewriteEngine On

# Handle root domain requests
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} ^/[A-Z0-9]{6}/?$
RewriteRule ^([A-Z0-9]{6})/?$ /unique-pic-id/index.html [L]

# Handle other routes
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /index.html [L]
